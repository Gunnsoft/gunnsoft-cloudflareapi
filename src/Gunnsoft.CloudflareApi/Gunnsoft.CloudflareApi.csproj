<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
  </PropertyGroup>

  <PropertyGroup>
    <Title>Gunnsoft.CloudflareApi</Title>
    <AssemblyName>Gunnsoft.CloudflareApi</AssemblyName>
    <PackageId>Gunnsoft.CloudflareApi</PackageId>
    <Authors>Andrew Gunn</Authors>
    <Company>Gunnsoft</Company>
    <AssemblyTitle>Gunnsoft.CloudflareApi</AssemblyTitle>
    <Description>
      Gunnsoft Cloudflare API
    </Description>
    <Copyright>Copyright Â© Gunnsoft 2018</Copyright>
    <PackageTags>Gunnsoft;Cloudflare</PackageTags>
    <PackageProjectUrl>https://github.com/Gunnsoft/gunnsoft-cloudflareapi</PackageProjectUrl>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/Gunnsoft/gunnsoft-cloudflareapi</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Autofac" Version="4.6.2" />
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="2.0.0" />
    <PackageReference Include="Newtonsoft.Json" Version="11.0.1" />
  </ItemGroup>

</Project>
